@model IEnumerable<PartTimeJob.Models.EnrollmentWithOutQualificationJob>

@{
    ViewBag.Title = "My Enrollment Job List";
}

<h2>List of Enrollments</h2>

<p>@*
    @Html.ActionLink("Apply New", "Create")
*@
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Employee.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WithOutQualificationJob.JobName)
        </th>
        <th>
            Enrollment Date
            @*
    @Html.DisplayNameFor(model => model.Date)*@
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Employee.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WithOutQualificationJob.JobName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Date)
        </td>
        <td>
            @*@Html.ActionLink("Edit", "Edit", new { id=item.Id }) |*@
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>

<div>
    @Html.ActionLink("Back to Your Home Page", "Home","EmployeeHomaPage", new { area = "" })
</div>